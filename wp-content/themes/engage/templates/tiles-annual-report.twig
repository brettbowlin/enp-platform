<section class="ann__sct ann__sct_tiles wrapper--pad-xlg">
    <div class="ann__tile">
        <div class="ann__img-wrap">
            <img src="{{ post.box_left_image_05.src }}" class="ann__tile_img"
                sizes="{{ post.box_left_image_05.img_sizes }}" alt="{{ post.box_left_image_05.alt }}" />
        </div>
        <span class="sr-text">
            <h2>{{ post.meta('box_left_title_05') }}</h2>
            <p>{{ post.meta('box_left_text_05') }}</p>
        </span>
        <div class="ann__info" style="background-color: {{ post.meta('box_left_color_05') }}">
            <h2 class="ann__title">{{ post.meta('box_left_title_05') }}</h2>
            <p class="ann__excerpt">{{ post.meta('box_left_text_05') }}</p>
            <p>Read more &#8250 &#8250</p>
        </div>
        <div class="ann__hover_more">
            {% if post.meta('box_left_hover_title_01_05') %}
