{% extends "base.twig" %}

{% block content %}

<div class="container_team">
	{% for category in categories %}
		{% if category != 'alumni' %}
			<h2 class="category_title">{{ category_titles[category] }}</h2>
		{% endif %}
		{% include 'partial/teammates_new.twig' with { mates: filtered_posts[category], format: "excerpt", category: category } %}
	{% endfor %}
</div>
{% endblock %}